{
  "name": "LinkedIn Modal Remover 2",
  "description": "Watch linkedin profile without login. Linkedin popup remover.",
  "version": "1.0.2",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["/assets/js/jQuery.js","background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "LinkedIn Modal Remover",
    "default_icon": "/assets/logo/logom.png"
  },
 "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["/assets/js/jQuery.js","content_script.js"]
    }
  ],
  "manifest_version": 2
}
